# why would you do anything else?
options(stringsAsFactors = FALSE)

# load colorout for use with nvim-R
library(colorout)
# reasonable defaults for colorout
setOutputColors(normal = 4, negnum = 4, zero = 4, number = 4, date = 4, string = 4, const = 4, false = 4, true = 4, infinite = 4, index = 4, stderror = 4, warn = 3, error = 1)


